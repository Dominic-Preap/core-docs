{
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    ".nyc_output": true,
    ".vscode": false,
    "coverage": true,
    "dist": true,
    "node_modules": true,
    ".docusaurus": true
  },
  "git.autofetch": true,
  "typescriptHero.imports.multiLineTrailingComma": false,
  "typescriptHero.imports.multiLineWrapThreshold": 120,
  "typescriptHero.imports.removeTrailingIndex": true,
  "typescript.tsdk": "node_modules/typescript/lib",
  "search.exclude": {
    "**/.docusaurus": true,
    "**/node_modules": true,
    "**/bower_components": true,
    "**/dist": true
  },
  "cSpell.words": [
    "sendbird",
    "twilio",
    "typeorm"
  ]
}
